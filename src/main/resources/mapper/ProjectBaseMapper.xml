<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo0720.mapper.ProjectBaseMapper">
    <select id="showProjectBase" resultType="map">
        select project_name,project_area,build_status
        from project_base
        where project_sn=#{projectCode}
    </select>
</mapper>